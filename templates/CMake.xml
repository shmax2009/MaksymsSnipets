<templateSet group="CMake">
  <template name="exe" value="add_executable($TARGET$ $SOURCES$)" description="Add new executable target" toReformat="true" toShortenFQNames="true">
    <variable name="TARGET" expression="" defaultValue="&quot;TargetName&quot;" alwaysStopAt="true" />
    <variable name="SOURCES" expression="complete()" defaultValue="" alwaysStopAt="true" />
  </template>
  <template name="lib" value="add_library($TARGET$ $SOURCES$)" description="Add new library target" toReformat="true" toShortenFQNames="true">
    <variable name="TARGET" expression="" defaultValue="&quot;TargetName&quot;" alwaysStopAt="true" />
    <variable name="SOURCES" expression="complete()" defaultValue="" alwaysStopAt="true" />
  </template>
</templateSet>